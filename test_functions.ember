function add(a, b) {
    return a + b
}

result = add(5, 3)
print("add(5, 3) =", result)

function factorial(n) {
    if n <= 1 {
        return 1
    } else {
        return n * factorial(n - 1)
    }
}

fact5 = factorial(5)
print("factorial(5) =", fact5)

