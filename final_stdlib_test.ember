print("=== Final Comprehensive Standard Library Test ===")

# Math functions
print("Math: abs(-42) =", abs(-42))
print("Math: sqrt(144) =", sqrt(144))
print("Math: pow(2, 8) =", pow(2, 8))

# Crypto functions  
print("Crypto: sha256('hello world') =", sha256("hello world"))
print("Crypto: sha512('test') =", sha512("test"))
print("Crypto: secure_random(8) =", secure_random(8))

# String functions
test_str = "Hello, Ember Platform!"
print("String: len(test_str) =", len(test_str))
print("String: substr(test_str, 7, 5) =", substr(test_str, 7, 5))
words = split(test_str, " ")
print("String: split result =", words)
print("String: join result =", join(words, "-"))

# Type conversion
print("Convert: str(3.14159) =", str(3.14159))
print("Convert: num('42.5') =", num("42.5"))
print("Convert: bool(1) =", bool(1))
print("Convert: bool(0) =", bool(0))

# JSON functions
print("JSON: json_stringify('test') =", json_stringify("test"))
print("JSON: json_stringify(123) =", json_stringify(123))
print("JSON: json_validate('{}') =", json_validate("{}"))

# File I/O
print("File: file_exists('/tmp') =", file_exists("/tmp"))
print("File: file_exists('/nonexistent') =", file_exists("/nonexistent"))

print("=== All Standard Library Functions Working! ===")